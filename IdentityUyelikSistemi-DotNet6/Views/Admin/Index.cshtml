@model List<AppUser>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<table class="table table-bordered table-striped">
    <tr>
        <td>User Id</td>
        <td>User Name</td>
        <td>Email</td>
    </tr>

    @if (Model.Count()==0)
    {
        <tr>
            <td colspan="3">Hiç Bir Üye Yoktur.</td>

        </tr>
    }
    else
    {
        @foreach (var item in Model)
        {
            <tr>
                <th>@item.Id</th>
                <th>@item.UserName</th>
                <th>@item.Email</th>
            </tr>
        }
    }
</table>

